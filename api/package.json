{
  "name": "grand-stack-starter-api",
  "version": "2.0.0",
  "description": "API app for GRANDstack",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "./node_modules/.bin/nodemon --watch src --ext js,graphql --delay 3 --exec babel-node  src/index.js",
    "build": "babel src --out-dir build && shx cp .env build 2>/dev/null || : && shx cp -r src/schema build",
    "now-build": "babel src --out-dir build && shx cp -r src/schema build",
    "start": "npm run build && node build/index.js",
    "seedDb": "./node_modules/.bin/babel-node src/seed/seed-db.js"
  },
  "author": "William Lyon",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@neo4j/graphql": "^3.0.4",
    "@neo4j/graphql-plugin-auth": "^1.0.0",
    "apollo-server": "^3.5.0",
    "apollo-server-lambda": "^3.5.0",
    "csv-parse": "^5.0.3",
    "dotenv": "^10.0.0",
    "form-data": "^4.0.0",
    "graphql": "^16.3.0",
    "mailgun.js": "^3.7.3",
    "neo4j-driver": "^4.3.1",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs3": "^7.9.2",
    "babel-plugin-auto-import": "^1.0.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.15",
    "shx": "^0.3.2"
  }
}
