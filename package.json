{
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "concurrently": "^6.5.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "execa": "^4.0.2",
    "generate-changelog": "^1.8.0",
    "grandstack": "^0.1.7",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.3",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0"
  },
  "scripts": {
    "start": "node scripts/start-dev.js",
    "build": "node scripts/build.js",
    "format": "find . -name \"*.js\" | grep -v node_modules | grep -v build | xargs prettier --write",
    "format:log": "find . -name \"*.js\" | grep -v node_modules | grep -v build | xargs prettier",
    "inferschema:write": "node scripts/inferSchema.js",
    "release:major": "node scripts/release.js major",
    "release:minor": "node scripts/release.js minor",
    "release:patch": "node scripts/release.js patch",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "eslintIgnore": [
    "*.test.js",
    "*.test.jsx"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/firstlovecenter/fl-admin-portal.git"
  },
  "license": "Apache-2.0",
  "version": "2.1.0",
  "dependencies": {
    "neo4j-driver": "^4.4.5"
  }
}
